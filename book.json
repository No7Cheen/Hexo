{
    "title": "Hexo 简易教程",
    "description": "记录 Hexo 的一些基本操作",
    "author": "rogers cj",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.2",
    "root": ".",
    "structure": {
        "readme": "introduction.md"
    },
    "links": {
        "sidebar": {
            "Home": "https://www.gitbook.com/@rogerscj"
        }
    },
    "plugins": [
        "-search",
        "-sharing",
        "-highlight",
        "sharing-plus",
        "search-pro",
        "search-plus",
        "github",
        "github-buttons",
        "tbfed-pagefooter",
        "donate",
        "disqus",
        "splitter",
        "anchors",
        "anchor-navigation-ex",
        "sectionx",
        "toggle-chapters",
        "ace",
        "mermaid",
        "mathjax",
        "emphasize",
        "codeblock-filename",
        "include-codeblock",
        "local-video"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "fontsettings": {
            "theme": "white",
            "family": "serif",
            "size": 2
        },
        "sharing": {
            "qq": true,
            "qzone": true,
            "weibo": true,
            "douban": true,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "whatsapp": false,
            "all": [
                "qq",
                "qzone",
                "weibo",
                "douban",
                "google"
            ]
        },
        "github": {
            "url": "https://github.com/rogerscj"
        },
        "github-buttons": {
            "repo": "rogerscj/Hexo",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © rogers 2017",
            "modify_label": "修订时间：",
            "modify_format": "YYYY-MM-DD"
        },
        "donate": {
            "wechat": "https://kiwigo.com/resource/weixin.png",
            "alipay": "https://kiwigo.com/resource/alipay.png",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "disqus": {
            "shortName": "hexouse"
        },
        "anchor-navigation-ex": {
            "associatedWithSummary": true,
            "showLevel": false,
            "multipleH1": true,
            "mode": "float",
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "sectionx": {
            "tag": "b"
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "edit": true
        }
    }
}