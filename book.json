{
    "title": "Hexo 简易教程",
    "description": "记录 Hexo 的一些基本操作",
    "author": "rogers cj",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.2",
    "root": ".",
    "structure": {
        "readme": "introduction.md"
    },
    "links": {
        "sidebar": {
            "Home": "https://www.gitbook.com/@rogerscj"
        }
    },
    "plugins": [
        "-sharing",
        "-highlight",
        "sharing-plus",
        "github",
        "github-buttons",
        "donate",
        "disqus"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "fontsettings": {
            "theme": "white",
            "family": "serif",
            "size": 2
        },
        "sharing": {
            "qq": true,
            "qzone": true,
            "weibo": true,
            "douban": true,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "whatsapp": false,
            "all": [
                "qq",
                "qzone",
                "weibo",
                "douban",
                "google"
            ]
        },
        "github": {
            "url": "https://github.com/rogerscj"
        },
        "github-buttons": {
            "repo": "rogerscj/Hexo",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "donate": {
            "wechat": "https://kiwigo.com/resource/weixin.png",
            "alipay": "https://kiwigo.com/resource/alipay.png",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "disqus": {
            "shortName": "hexouse"
        }
    }
}